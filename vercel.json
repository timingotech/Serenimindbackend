{
    "version": 2,
    "builds": [{
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "50mb",
            "runtime": "python3.12"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ],
    "functions": {
        "backend/wsgi.py": {
            "maxDuration": 60,
            "memory": 1024      
        }
    }
}